func fizzbuzz(n)
->
    func step(numb, goal)
    ->
        var nu = numb + 1
        if nu <= goal
        ->

            if nu % 3 -> print("Fizz")
            if nu % 5 -> print("Buzz")

            if not nu % 3 
            ->
                if not nu % 5
                ->
                    print(nu)

            print()

            step(nu, goal)

    step(0)

